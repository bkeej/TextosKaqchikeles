            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a122" ANNOTATION_REF="a107">
                <ANNOTATION_VALUE>Esos son los pescaditos que comemos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a123" ANNOTATION_REF="a108">
