        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a88" ANNOTATION_REF="a46">
                <ANNOTATION_VALUE>Le hechamos cebolla.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a89" ANNOTATION_REF="a47">
                <ANNOTATION_VALUE>Le hechamos cebolla y  tomate.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a90" ANNOTATION_REF="a48">
                <ANNOTATION_VALUE>Y se coce en unos diez o quince minutos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a91" ANNOTATION_REF="a49">
                <ANNOTATION_VALUE>Cuando se coce nuestro tomate, entonces lo sacamos,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a92" ANNOTATION_REF="a50">
                <ANNOTATION_VALUE>Entonces lo machacamos,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a93" ANNOTATION_REF="a51">
                <ANNOTATION_VALUE>Lo machacamos, el tomate, y entonces lo hechamos también dentro de nuestra olla.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a94" ANNOTATION_REF="a52">
                <ANNOTATION_VALUE>Y entonces si hemos traído nuestro pescadito, eso, y ya seleccionado nuestro pescadito.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a53">
                <ANNOTATION_VALUE>Si ya lo hemos seleccionado (el pescadito) hasta entonces lo hechamos en el caldito de tomate,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a96" ANNOTATION_REF="a54">
                <ANNOTATION_VALUE>Hierve unos diez minutos o quince minutos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a97" ANNOTATION_REF="a55">
                <ANNOTATION_VALUE>Eso, el el, eso ya terminamos una nuestra comida.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a56">
                <ANNOTATION_VALUE>Un chirmol, le decimos a eso o tomate  en machacado o pescadito en machacado tomate.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a99" ANNOTATION_REF="a57">
                <ANNOTATION_VALUE>Eso.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a100" ANNOTATION_REF="a58">
                <ANNOTATION_VALUE>Y el otro el, que le decimos, nuestro chirmol le decimos,</ANNOTATION_VALUE>
