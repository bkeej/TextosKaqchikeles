        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a115" ANNOTATION_REF="a76">
                <ANNOTATION_VALUE>Más lo que lleva eso es el chile.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a116" ANNOTATION_REF="a77">
                <ANNOTATION_VALUE>Pero para eso todos más para nuestro jocón (salsa de tomate) le hechamos ajo sobre (el jocón). </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a117" ANNOTATION_REF="a78">
                <ANNOTATION_VALUE>Eso.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a118" ANNOTATION_REF="a79">
                <ANNOTATION_VALUE>Y en eso le hechamos nuestro pescadito adentro y lo comemos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a119" ANNOTATION_REF="a80">
                <ANNOTATION_VALUE>Eso es la comida de comer pescadito (seco). </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a120" ANNOTATION_REF="a81">
