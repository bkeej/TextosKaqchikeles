        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a85" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE>Bueno, aquí pues  hacemos su forma, le hacemos a los pescaditos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a86" ANNOTATION_REF="a44">
                <ANNOTATION_VALUE>Si lo hacemos en molido tomate,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
